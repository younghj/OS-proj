/** 
 * Multiplexor. If sel==1 then out=b else out=a.
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel,out=nsel);
	And(a=a,b=nsel,out=r1);
	And(a=sel,b=b,out=r2);
	Or(a=r1,b=r2,out=out);
}